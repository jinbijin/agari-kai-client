{
  "name": "agari-kai-client",
  "version": "0.0.0",
  "engines": {
    "node": "^16.0.0",
    "npm": "^8.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:ci": "ng build --configuration development",
    "build:prod": "ng build --configuration production",
    "build:analysis": "ng build --configuration source-map-analysis",
    "watch": "ng build --watch --configuration development",
    "watch:prod": "ng build --watch --configuration production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "test:coverage": "jest --coverage",
    "format:check": "prettier . --check",
    "format:fix": "prettier . --write",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cypress:open": "ng run agari-kai-client:cypress-open",
    "cypress:run": "ng run agari-kai-client:cypress-run",
    "serve:static": "http-server -p 8080 -c-1 dist/agari-kai-client",
    "analyze-source-map": "source-map-explorer dist/agari-kai-client/**/*.js"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ],
    "globalSetup": "jest-preset-angular/global-setup",
    "moduleNameMapper": {
      "src/(.*)": "<rootDir>/src/$1"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.5",
    "@angular/cdk": "^14.2.4",
    "@angular/common": "^14.2.5",
    "@angular/compiler": "^14.2.5",
    "@angular/core": "^14.2.5",
    "@angular/forms": "^14.2.5",
    "@angular/material": "^14.2.4",
    "@angular/platform-browser": "^14.2.5",
    "@angular/platform-browser-dynamic": "^14.2.5",
    "@angular/router": "^14.2.5",
    "@angular/service-worker": "^14.2.5",
    "dexie": "^3.2.2",
    "luxon": "^3.0.4",
    "luxon-angular": "^5.1.0",
    "rxjs": "~7.5.7",
    "tslib": "^2.3.0",
    "uuid": "^9.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.5",
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/schematics": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "~14.2.5",
    "@angular/compiler-cli": "^14.2.5",
    "@cypress/schematic": "^2.1.1",
    "@types/jest": "^28.1.8",
    "@types/luxon": "^3.0.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "cypress": "^10.9.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "http-server": "^14.1.1",
    "jest": "^28.1.3",
    "jest-preset-angular": "^12.2.2",
    "prettier": "^2.7.1",
    "source-map-explorer": "^2.5.3",
    "typescript": "~4.7.2"
  }
}
